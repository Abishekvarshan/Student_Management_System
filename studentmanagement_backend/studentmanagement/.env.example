# Copy this file to `.env` and fill in real values.
#
# IMPORTANT:
# - Do NOT commit `.env` to git.
# - Prefer using strong secrets for JWT.

# Supabase API (if you use SupabaseConfig)
SUPABASE_URL=https://<your-project>.supabase.co/rest/v1/students
SUPABASE_KEY=<your-supabase-api-key>

# Database
SPRING_DATASOURCE_URL=jdbc:postgresql://<host>:<port>/<db>
SPRING_DATASOURCE_USERNAME=<username>
SPRING_DATASOURCE_PASSWORD=<password>

# JPA
SPRING_JPA_SHOW_SQL=true
SPRING_JPA_HIBERNATE_DDL_AUTO=update
SPRING_JPA_PROPERTIES_HIBERNATE_DIALECT=org.hibernate.dialect.PostgreSQLDialect

# JWT
SECURITY_JWT_SECRET=change-this-secret-to-a-long-random-string
SECURITY_JWT_EXPIRATION_MS=86400000
